include _config
include _mixins

block vars
    - metas = {}
    - metas.title = site.name
    - metas.url = siteurl
    - metas.image = 'imageimage'
    - metas.site_name = site.name
    - metas.description = sitedescription
    - ga_id = google_analytics_id
    - pageClass = ""
doctype html
html(lang="ja")
  include _meta
  body(data-page=page)
    //- include _gtm-noscript
    #wrapper
      include _header
      block contents
      include _footer
    include _foot
